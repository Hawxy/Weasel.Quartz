<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    
    <PropertyGroup>
        <PackageId>Weasel.Quartz.Postgres</PackageId>
        <Description>Runtime migrations for Quartz.NET Postgres Schema</Description>
        <PackageTags>weasel,quartz,postgres</PackageTags>
        <Version>3.0.0</Version>
        <Authors>Hawxy</Authors>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/Hawxy/Weasel.Quartz</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Hawxy/Weasel.Quartz</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Copyright>Hawxy 2024</Copyright>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Weasel.Postgresql" Version="7.4.1" />
    </ItemGroup>

</Project>
